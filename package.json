  "scripts": {
    "prepare": "husky install",
    "commit": "git-cz",
    "lint:fix": "nx affected --target=lint --fix --base=master",
    "nx:graph": "nx affected:graph --base=master --only-build --quiet",
    "setup": "npm exec -- rimraf node_modules/@template-monorepo && npm exec -- rimraf node_modules/storybook-static && npm i && npm exec -- cross-env-shell && patch-package && npm run prepare && npm exec -- msw init public -y"
  },
  "config": {
    "commitizen": {
      "path": "./scripts/commitizen"
    }
  },
  "msw": {
    "workerDirectory": "public"
  },
